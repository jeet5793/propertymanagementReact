}
.radio-dot:before, .radio-dot:after {
	content: "";
	display: block;
	position: absolute;
	background: #fff;
	border-radius: 100%;
}
.radio-dot:before {
	width: 20px;
	height: 20px;
	border: 1px solid #ccc;
	top: 6px;
	left: 16px;
}
.radio-dot:after {
	width: 12px;
	height: 12px;
	border-radius: 100%;
	top: 10px;
	left: 20px;
}
.buying-selling.active .buying-selling-word {
	color: #fff;
}
.buying-selling.active .radio-dot:after {
	background: #426C2A;
}
.buying-selling.active .radio-dot:before {
	background: #fff;
	border-color: #699D17;
}
.buying-selling:hover .radio-dot:before {
	border-color: #adadad;
}
.buying-selling.active:hover .radio-dot:before {
	border-color: #699D17;
}
.buying-selling.active .radio-dot:after {
	background: #426C2A;
}
.buying-selling:hover .radio-dot:after {
	background: #e6e6e6;
}
.buying-selling.active:hover .radio-dot:after {
	background: #426C2A;
}

